aws sqs list-queues

aws sqs send-message --queue-url https://sqs.us-east-1.amazonaws.com/338746472000/alura-test --message-body "Ola, mundo"

aws sqs receive-message --queue-url https://sqs.us-east-1.amazonaws.com/338746472000/alura-test

aws sqs create-queue --queue-name alura-test

aws sqs delete-queue --queue-url https://sqs.us-east-1.amazonaws.com/338746472000/alura-test

aws sqs delete-message --receipt-handle  --queue-url https://sqs.us-east-1.amazonaws.com/338746472000/alura-test

aws sqs receive-message --queue-url https://sqs.us-east-1.amazonaws.com/338746472000/alura-test --wait-time-seconds 20